{
  "expo": {
    "name": "Mood Lighting",
    "description": "Wifi Controlled Mood Lighthing",
    "slug": "mood-lighting",
    "privacy": "public",
    "sdkVersion": "24.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.bhaktij.moodlightning",
      "supportsTablet": true
    },
    "android": {
      "package": "com.bhaktij.moodlightning",
      "permissions" : [ "READ_INTERNAL_STORAGE" ],
    }
  }
}
